<section class="h-full pt-20 flex flex-row gap-6">
  <div class="w-full">
    <h1 class="text-6xl font-normal leading-snug">
      Flowers, <img src="/assets/flower.svg" alt="Flower Icon" class="inline-block" /> 
      <br /> what the world <br/> needs 
    </h1>

    <p class="mt-9 text-xl text-zinc-500">Browse between hounders of flowers</p>

    <button 
      type="button" 
      class="mt-11 py-2 px-7 rounded-md bg-orange-400 transition ease-in-out duration-200 hover:bg-orange-500 text-zinc-50"
    >
      Browse
    </button>
  </div>

  <div class="w-full">
    <div class="grid grid-cols-3 grid-rows-2 gap-5">
      <div class="overflow-hidden" style="max-width: 192px; max-height: 192px;">
        <img src="https://images.unsplash.com/photo-1610397648930-477b8c7f0943?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=200&q=80" alt="Flower" class="object-cover w-full h-full rounded-xl" >
